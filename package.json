{
  "name": "dota-help-backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Heytor Cavalcanti <heytor.c.moura@accenture.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "tsc && node build/server.js"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-steam-openid": "^1.0.0",
    "socket.io": "^4.3.2",
    "ts-node": "^10.4.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
